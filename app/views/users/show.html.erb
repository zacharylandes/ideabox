<nav>
<h1>Welcome, <%= current_user.username %>!</h1>
</nav>
<%= link_to "Logout", logout_path %>
<div class="user-container">
      <div  class="idea-form">
      <h2>What's your idea?</h2>
        <%= render partial: "form" %>
      </div>

    <% if !@ideas.nil? %>
      <% @ideas.each do |idea| %>
      <div class= "cat-show">
        <p> Idea: <%= idea.title  %></p>
        <p> <%= idea.description  %></p>
        <p>Category: <%= idea.category.name  %></p>
        <p><%= link_to "Edit", edit_user_idea_path(@user, idea) %></p>
           <%= link_to "Delete", user_idea_path(@user, idea), method: "delete" %>
          <%idea.images.each do |image| %>
            <p>  <%= image.name %></p>
              <%=image_tag image.image.url unless image.image.url.nil? %>
          <% end %>
          <%idea.gifs.each do |gif| %>
            <p>  <%= gif.name %></p>
              <img src= "<%=gif.url%>">
          <% end %>
      </div>
      <% end %>
    </div>
      <% end %>
    </div>
