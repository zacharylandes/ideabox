<nav>

  <h1>Sign Up</h1>
    <%= link_to "Home", logout_path, class: "logout" %>

</nav>
<div class = "idea-form">
<%= form_for @user do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.submit %>
<% end %>
</div>
